<jboss-deployment-structure>
	<ear-subdeployments-isolated>true</ear-subdeployments-isolated>
	<deployment>
		<dependencies>
			<module name="org.slf4j" slot="main" />
			<module name="org.hibernate" slot="main" />
			<module name="org.hibernate.envers" slot="main" />
			<module name="org.infinispan" slot="main" export="true" />
			<module name="org.jboss.as.ejb3" slot="main" />
			<module name="org.jboss.as.web" slot="main" />
			<module name="org.apache.cxf" slot="main" export="true" />
			<module name="org.apache.cxf.impl" slot="main" export="true" />
			<module name="org.joda.time" slot="main" export="true" />
			<module name="org.yaml.snakeyaml" slot="main" export="true" />
			<module name="org.apache.commons.lang" slot="main" export="true" />
			<module name="org.apache.commons.io" slot="main" export="true" />
			<module name="com.google.guava" slot="main" export="true" />
			<module name="org.apache.httpcomponents" slot="main" export="true" />
			<system export="true">
				<paths>
					<path name="com/sun/net/ssl/internal/ssl" />
					<path name="com/sun/net/ssl" />
				</paths>
			</system>
		</dependencies>
	</deployment>
</jboss-deployment-structure>